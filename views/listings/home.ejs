      text-align: center;
    }

    /* Hero Section */

    .hero {
      position: relative;
      height: 600px;
      overflow: hidden;
    }

    .hero video {
      position: absolute;
      top: 50%;
      left: 50%;
      min-width: 100%;
      min-height: 100%;
      width: auto;
      height: auto;
      z-index: -100;
      transform: translateX(-50%) translateY(-50%);
      background-size: cover;
      transition: 1s opacity;
    }

    .hero .hero-content {
      position: relative;
      z-index: 1;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      height: 100%;
      color: white !important;
      text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5);
    }

    .hero h1 {
      font-size: 4rem;
      font-weight: bold;
      margin-bottom: 1rem;
    }

    .hero p {
      font-size: 1.5rem;
      margin-bottom: 2rem;
    }

    /* Featured Destinations */
    .featured-destinations {
      padding: 50px 0;
    }

    .destination-card {
      margin-bottom: 30px;
      position: relative;
      overflow: hidden;
    }

    .destination-card img {
      height: 300px;
      object-fit: cover;
      transition: transform 0.3s ease-in-out;
    }

    .destination-card:hover img {
      transform: scale(1.1);
    }

    .destination-card .overlay {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, 0.5) !important;
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      color: white !important;
      opacity: 0;
      transition: opacity 0.3s ease-in-out;
    }

    .destination-card:hover .overlay {
      opacity: 1;
    }

    .overlay h4 {
      font-size: 1.8rem;
      font-weight: bold;
      margin-bottom: 0.5rem;
    }

    .overlay p {
      font-size: 1.2rem;
      margin-bottom: 1rem;
    }

    .overlay a {
      color: white !important;
      font-size: 1.2rem;
